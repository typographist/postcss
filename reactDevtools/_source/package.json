{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start":
      "cross-env nodemon --watch ./config ./node_modules/.bin/webpack-dev-server",
    "build":
      "npm run clean && cross-env NODE_ENV=production webpack -p && npm run demo",
    "demo": "cross-env static -a 0.0.0.0 dist",
    "clean": "cross-env rimraf dist",
    "unit:watch": "cross-env jest --watch"
  },
  "jest": {
    "collectCoverage": true,
    "transform": { "^.+\\.js$": "./node_modules/babel-jest" },
    "testFileExtensions": ["es6", "js"],
    "moduleFileExtensions": ["js", "json", "es6"]
  },
